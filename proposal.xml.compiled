                            <choice xmlns="http://relaxng.org/ns/structure/1.0"><oneOrMore><ref name="model.correspDescPart"/></oneOrMore><oneOrMore><ref name="model.pLike"/></oneOrMore></choice>
                            <optional xmlns="http://relaxng.org/ns/structure/1.0"><ref name="model.noteLike"/></optional>
