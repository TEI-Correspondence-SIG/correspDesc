                            <memberOf key="att.global"/>
                            <memberOf key="att.typed"/>
                            <memberOf key="att.responsibility"/>
                            <memberOf key="att.personal"/>
                            
                            <memberOf key="model.correspDescPart"/>
                        </classes><content>
                            <choice xmlns="http://relaxng.org/ns/structure/1.0"><optional><text/></optional><group><choice><ref name="model.nameLike.agent"/><oneOrMore><ref name="model.pLike"/></oneOrMore></choice><optional><ref name="model.noteLike"/></optional></group></choice>
                        </content><attList/><exemplum xml:lang="en">
